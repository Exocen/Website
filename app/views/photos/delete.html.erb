<% provide(:title,"Photo") %>
<h1>Delete Photos</h1>
<div class="panel-body">
<% @photos.each do |photo| %>
<%= link_to image_tag(photo.image.url(:small)),photo_path(photo), method: :delete, :class => 'thumbnail' %>
<% end %>
</div>
<button type="button" class="btn btn-success btn-circle btn-lg"
data-toggle="tooltip" data-placement="top" title="Validate !">
<%= link_to 'photo', class: 'button alt' do %>
<i class="glyphicon glyphicon-ok"></i>
<% end %>
</button>
